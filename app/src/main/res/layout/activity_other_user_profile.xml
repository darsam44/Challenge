<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Acvtivities.Activities.Other_user_profile"
    android:background="@drawable/gradient_background">


    <TextView
        android:id="@+id/O_Email"
        android:layout_width="120dp"
        android:layout_height="29dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:text="Email:"
        android:textColor="#9C27B0"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/O_Email_p"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_Last_name" />

    <TextView
        android:id="@+id/O_Phone"
        android:layout_width="103dp"
        android:layout_height="29dp"
        android:layout_marginStart="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginBottom="260dp"
        android:text="Phone:"
        android:textColor="#9C27B0"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/O_Phone_p"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_Email" />

    <TextView
        android:id="@+id/O_Last_name"
        android:layout_width="120dp"
        android:layout_height="29dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:text="Last Name:"
        android:textColor="#9C27B0"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/O_UserName_p"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_Username" />

    <TextView
        android:id="@+id/O_Username"
        android:layout_width="120dp"
        android:layout_height="29dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="42dp"
        android:text="Username"
        android:textColor="#9C27B0"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/O_LastName_p"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_First_name" />

    <ImageView
        android:id="@+id/O_image_profile"
        android:layout_width="260dp"
        android:layout_height="166dp"
        android:layout_marginTop="36dp"
        android:scaleType="centerCrop"
        android:src="@drawable/profile_image"
        app:layout_constraintBottom_toTopOf="@+id/choose_ima"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.476" />

    <TextView
        android:id="@+id/O_FirstName_p"
        android:layout_width="140dp"
        android:layout_height="29dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="73dp"
        android:layout_marginRight="73dp"
        android:text="First Name"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_image_profile" />

    <TextView
        android:id="@+id/O_UserName_p"
        android:layout_width="140dp"
        android:layout_height="29dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="73dp"
        android:layout_marginRight="73dp"
        android:text="User Name"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_LastName_p" />

    <TextView
        android:id="@+id/O_Phone_p"
        android:layout_width="140dp"
        android:layout_height="29dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="73dp"
        android:layout_marginRight="73dp"
        android:layout_marginBottom="260dp"
        android:text="Phone"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_Email_p" />

    <TextView
        android:id="@+id/O_LastName_p"
        android:layout_width="140dp"
        android:layout_height="29dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="72dp"
        android:layout_marginRight="72dp"
        android:text="Last Name"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_FirstName_p" />

    <TextView
        android:id="@+id/O_Email_p"
        android:layout_width="140dp"
        android:layout_height="29dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="73dp"
        android:layout_marginRight="73dp"
        android:text="Email"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_UserName_p" />

    <TextView
        android:id="@+id/O_First_name"
        android:layout_width="120dp"
        android:layout_height="29dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:text="First Name:"
        android:textColor="#9C27B0"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/O_FirstName_p"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/O_image_profile" />


</androidx.constraintlayout.widget.ConstraintLayout>